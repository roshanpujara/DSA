<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <script>
      // Inserting Element in array without using any js function.
        let arr = [1, 2, 3, 5, 6, 7, 8, 9];
        let newValue = 4;
        let changeIndex = 3;

        for (let i = arr.length - 1; i >= 0; i--) {
            if (i >= changeIndex) {
                arr[i + 1] = arr[i];
                if (i == changeIndex) {
                    arr[i] = newValue;
                }
            }
        }
        document.write('roshan')
        document.write(arr)


    </script>
</body>

</html>
